#include <bits/stdc++.h>
using namespace std;

int main(){
	ios::sync_with_stdio(0);
	cin.tie(0);
	int a,x,y;
	cin>>a;
	for(int i=0;i<a;i++){
		cin>>x>>y;
		for(int j=0;j<y;j++){
			for(int k=0;k<=x;k++){
				for(int n=1;n<=k;n++){
					cout<<k;
				}
				cout<<'\n';
			}
			for(int m=x-1;m>0;m--){
				for(int n=1;n<=m;n++){
					cout<<m;
				}
					cout<<'\n';
			}
		}
	}
	return 0;
}
